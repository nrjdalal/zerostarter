{
  "name": "@packages/env",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./api-hono": {
      "types": "./dist/api-hono.d.mts",
      "default": "./dist/api-hono.mjs"
    },
    "./auth": {
      "types": "./dist/auth.d.mts",
      "default": "./dist/auth.mjs"
    },
    "./db": {
      "types": "./dist/db.d.mts",
      "default": "./dist/db.mjs"
    },
    "./web-next": {
      "types": "./dist/web-next.d.mts",
      "default": "./dist/web-next.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc --noEmit",
    "lint": "oxlint"
  },
  "prettier": {
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ],
    "printWidth": 100,
    "semi": false
  },
  "dependencies": {
    "@infisical/sdk": "catalog:",
    "@t3-oss/env-core": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@packages/tsconfig": "workspace:*",
    "@types/bun": "catalog:",
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
